<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RegisterActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/lname_field"
            android:hint="@string/lnamefield"
            android:autofillHints=""
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="81dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/fname_field"
            android:hint="@string/fnamefield"
            android:autofillHints=""
            android:layout_marginTop="21dp"
            app:layout_constraintTop_toBottomOf="@+id/lname_field" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/sname_field"
            android:hint="@string/snamefield"
            android:importantForAutofill="no"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/fname_field" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="74dp"
            android:layout_height="38dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/passport_field"
            android:hint="@string/seriesfield"
            android:importantForAutofill="no"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/dateofbirth_field"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/phone_field"
            android:hint="@string/phonehint"
            android:autofillHints=""
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/editText4" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/pass_field"
            android:hint="@string/passwordfield"
            android:importantForAutofill="no"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/phone_field" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/rpass_field"
            android:hint="@string/repeatpassfield"
            android:importantForAutofill="no"
            app:layout_constraintStart_toStartOf="@+id/pass_field" android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/pass_field"/>
    <TextView
            android:text="@string/fnamefield"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="48dp"
            android:layout_marginStart="17dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"/>
    <TextView
            android:text="@string/snamefield"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="44dp"
            android:layout_marginStart="17dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"/>
    <TextView
            android:text="@string/dateofbirthfield"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/dateofbirth_field" android:layout_marginStart="17dp"/>
    <TextView
            android:text="@string/passport"
            android:layout_width="0dp"
            android:layout_height="36dp"
            android:id="@+id/textView5"
            android:textAlignment="viewStart" app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="17dp" android:layout_marginEnd="22dp"/>
    <TextView
            android:text="@string/phonehint"
            android:layout_width="0dp"
            android:layout_height="22dp"
            android:id="@+id/textView6"
            android:layout_marginTop="55dp"
            android:layout_marginBottom="57dp" app:layout_constraintEnd_toEndOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/textView5" app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintBottom_toBottomOf="@+id/textView7"/>
    <TextView
            android:text="@string/passwordhint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginBottom="71dp"
            app:layout_constraintBottom_toTopOf="@+id/register_button"/>
    <TextView
            android:text="@string/repeatpassfield"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="26dp"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"/>
    <TextView
            android:text="@string/lnamefield"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="89dp" android:layout_marginStart="17dp"
            app:layout_constraintBottom_toBottomOf="@+id/fname_field" android:layout_marginBottom="84dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/dateofbirth_field"
            android:hint="@string/dateofbirthfield"
            android:autofillHints=""
            android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/sname_field" android:layout_marginEnd="17dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="@string/register_button"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:id="@+id/register_button"
            android:layout_marginTop="131dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginBottom="134dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="38dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText4"
            android:hint="@string/numberfield"
            app:layout_constraintBaseline_toBaselineOf="@+id/passport_field"
            app:layout_constraintStart_toEndOf="@+id/passport_field" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="10dp" android:layout_marginEnd="16dp"
            android:importantForAutofill="no"/>
    <TextView
            android:text="@string/_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13"
            app:layout_constraintBaseline_toBaselineOf="@+id/phone_field"
            app:layout_constraintEnd_toStartOf="@+id/phone_field" android:layout_marginEnd="11dp"
    />
</androidx.constraintlayout.widget.ConstraintLayout>