<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActiveTicketsActivity">

    <com.google.android.material.bottomnavigation.BottomNavigationView
            android:layout_width="0dp"
            android:layout_height="79dp"
            android:id="@+id/bottomNavigationView4" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="452dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView4" android:id="@+id/recyclerView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="У вас"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:id="@+id/textView17" android:layout_marginBottom="60dp"
            app:layout_constraintBottom_toTopOf="@+id/recyclerView4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="130dp"
            app:layout_constraintEnd_toStartOf="@+id/textView20" android:layout_marginEnd="15dp"
    />
    <TextView
            android:text="талонов"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:id="@+id/textView19" android:layout_marginBottom="60dp"
            app:layout_constraintBottom_toTopOf="@+id/recyclerView4"
            app:layout_constraintStart_toEndOf="@+id/textView20"
    />
    <TextView
            android:text="4"
            android:layout_width="0dp"
            android:layout_height="23dp"
            android:id="@+id/textView20" android:layout_marginBottom="60dp"
            app:layout_constraintBottom_toTopOf="@+id/recyclerView4"
            app:layout_constraintStart_toEndOf="@+id/textView17" app:layout_constraintEnd_toStartOf="@+id/textView19"
            android:layout_marginEnd="15dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>